{
    "manifest_version": 2,
    "name": "Frogges",
    "version": "0.1.0",
    "description": "Adds a Click for frogs button to your toolbar in the spirit of Tumblr.",
    "icons": {
        "48": "icons/frogge-48.png"
    },
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_icon": "icons/frogge-48.png",
        "default_title": "Click for frogs",
        "default_popup": "popup/frogge.html"
    },
    "web_accessible_resources": [
        "notes/CLow.mp3",
        "notes/D.mp3",
        "notes/E.mp3",
        "notes/F.mp3",
        "notes/G.mp3",
        "notes/A.mp3",
        "notes/B.mp3",
        "notes/CHigh.mp3",
        "icons/buttons/frogge-pink.png",
        "icons/buttons/frogge-green.png",
        "icons/buttons/frogge-yellow.png",
        "icons/buttons/frogge-red.png"
    ]
}